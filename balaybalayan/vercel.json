{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": "create-react-app",
  "routes": [
    { "src": "^/static/(.*)", "dest": "/static/$1" },
    { "src": "^/favicon.ico", "dest": "/favicon.ico" },
    { "src": "^/manifest.json", "dest": "/manifest.json" },
    { "src": "^/(.*)js", "dest": "/$1js" },
    { "src": "^/(.*)css", "dest": "/$1css" },
    { "src": ".*", "dest": "/index.html" }
  ]
}